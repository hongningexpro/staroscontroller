<starlang xmlns="http://www.staros.xyz/starcore/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" scope="application">
	<!--定义：Policys-->
	<!--
		@brief		定义 Policys
		@auth 		qiulei
		@date		2018/01/08
		Input:
		Output: 
	-->
	<function name="AddDefinitionPolicysData">
		<string name="definitionname"/>
		<string name="definitioncontent"/>

		<set name="definitionname" value="'Policys'" />
		
		<reference variable="definitioncontent">
			<jsonbody><![CDATA[ 
				{
					"type": "object",
					"title": "policys",
					"required": [
						"policys",
						"resultCode",
						"resultDesc"
					],
					"properties": {
						"policys": {
							"type": "array",
							"xml": {
								"name": "policys",
								"wrapped": true
							},
							"items": {
								"type": "object",
								"title": "policy",
								"required": [
									"policyid",
									"srcIp",
									"dstIp",
									"content"
								],
								"properties": {
									"policyid": {
												"type": "string",
												"example": "3"
											},
									"srcIp": {
												"type": "string",
												"example": "192.168.137.128"
											},
									"dstIp": {
												"type": "string",
												"example": "192.168.137.129"
											},
									"content": {
												"type": "string",
												"example": "3"
											}
								}
							}
						},
						"resultCode": {
							"type": "string",
							"example": "1"
						},
						"resultDesc": {
							"type": "string",
							"example": "ok"
						}
					}
				}
			]]></jsonbody>
		</reference>
		
		<!--调用staros-controller-1.1.0\src\providers\CAPIDocsService.xml类中的方法,qiulei 20180109-->
		<function name="addDefinition" class="APIDocsService">
			<parameters>
				<string name="definitionname" in="context.definitionname.toString()"/>
				<string name="definitioncontent" in="context.definitioncontent.toString()"/>
			</parameters>
		</function>
		
	</function>
	
	<!--
		@brief		删除Policys
