<starlang xmlns="http://www.staros.xyz/starcore/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" scope="application">
	<!--定义：VGateways-->
	<!--
		@brief		定义 VGateways
		@auth 		qiulei
		@date		2018/01/10
		Input:
		Output: 
	-->
	<function name="AddDefinitionVGatewaysData">
		<string name="definitionname"/>
		<string name="definitioncontent"/>

		<set name="definitionname" value="'VGateways'" />
		
		<reference variable="definitioncontent">
			<jsonbody><![CDATA[ 
				{
					"type": "object",
					"title": "vGateways",
					"required": [
						"vGateways",
						"resultCode",
						"resultDesc"
					],
					"properties": {
						"vGateways": {
							"type": "array",
							"xml": {
								"name": "vGateways",
								"wrapped": true
							},
							"items": {
								"type": "object",
								"title": "vGateway",
								"required": [
									"ip",
									"netmask",
									"content"
								],
								"properties": {
									"ip": {
												"type": "string",
												"example": "192.168.137.128"
											},
									"netmask": {
												"type": "string",
												"example": "192.168.137.129"
											},
									"vmac": {
												"type": "string",
												"example": "3"
											}
								}
							}
						},
						"resultCode": {
							"type": "string",
							"example": "1"
						},
						"resultDesc": {
							"type": "string",
							"example": "ok"
						}
					}
				}
			]]></jsonbody>
		</reference>
		<string name="ip" />	
		<string name="netmask"/>
		<string name="vmac"/>
		<!--调用staros-controller-1.1.0\src\providers\CAPIDocsService.xml类中的方法,qiulei 20180109-->
		<function name="addDefinition" class="APIDocsService">
			<parameters>
				<string name="definitionname" in="context.definitionname.toString()"/>
				<string name="definitioncontent" in="context.definitioncontent.toString()"/>
			</parameters>
		</function>
		
	</function>
	
