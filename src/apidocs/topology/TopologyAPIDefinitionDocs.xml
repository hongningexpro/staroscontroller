	<!--定义：Topologys-->
	<function name="AddDefinitionTopologysData">
		<string name="definitionname"/>
		<string name="definitioncontent"/>

		<set name="definitionname" value="'Topologys'" />
		
		<reference variable="definitioncontent">
			<jsonbody><![CDATA[ 
				{
					"type": "object",
					"title": "topologys",
					"required": [
						"paths",
						"resultCode",
						"resultDesc"
					],
					"properties": {
						"paths": {
							"type": "array",
							"xml": {
								"name": "paths",
								"wrapped": true
							},
							"items": {
								"type": "object",
								"title": "topology",
								"required": [
									"dst",
									"src"
								],
								"properties": {
									"dst": {
										"type": "object",
										"title": "dst",
										"required": [
											"dpid",
											"port"
										],
										"properties": {
											"dpid": {
												"type": "string",
												"example": "2222"
											},
											"port": {
												"type": "string",
												"example": "3"
											}
										}
									},
									"src": {
										"type": "object",
										"title": "src",
										"required": [
											"dpid",
											"port"
										],
										"properties": {
											"dpid": {
												"type": "string",
												"example": "1111"
											},
											"port": {
												"type": "string",
												"example": "2"
											}
										}
									}
								}
							}
						},
						"resultCode": {
							"type": "string",
							"example": "1"
						},
						"resultDesc": {
							"type": "string",
							"example": "ok"
						}
					}
				}
			]]></jsonbody>
		</reference>
		
		<function name="addDefinition" class="APIDocsService">
			<parameters>
				<string name="definitionname" in="context.definitionname.toString()"/>
				<string name="definitioncontent" in="context.definitioncontent.toString()"/>
			</parameters>
		</function>
		
	</function>
	
	<function name="RemoveDefinitionTopologysData">
		<string name="definitionname"/>
		
		<set name="definitionname" value="'Topologys'" />

		<function name="removeDefinition" class="APIDocsService">
			<parameters>
				<string name="definitionname" in="context.definitionname.toString()"/>
			</parameters>
		</function>
	</function>


	<!--定义：Topology -->
	<function name="AddDefinitionTopologyData">
		<string name="definitionname"/>
		<string name="definitioncontent"/>
		
		<set name="definitionname" value="'Topology'" />
		
		<reference variable="definitioncontent">
			<jsonbody><![CDATA[ 
				{
		            "type": "object",
		            "title": "topology",
		            "required": [
						"path"
					],
					"properties": {
						"path": {
							"type": "object",
		            		"title": "topology",
		            		"required": [
								"dst",
								"src"
							],
							"properties": {
								"dst": {
									"type": "object",
									"title": "dst",
									"required": [
										"dpid",
										"port"
									],
									"properties": {
										"dpid": {
											"type": "string",
											"example": "2222"
										},
										"port": {
											"type": "string",
											"example": "3"
										}
									}
								},
								"src": {
									"type": "object",
									"title": "src",
									"required": [
										"dpid",
										"port"
									],
									"properties": {
										"dpid": {
											"type": "string",
											"example": "1111"
										},
										"port": {
											"type": "string",
											"example": "2"
										}
									}
								}
							}
						}
					}
				}
			]]></jsonbody>
		</reference>

		<function name="addDefinition" class="APIDocsService">
			<parameters>
				<string name="definitionname" in="context.definitionname.toString()"/>
				<string name="definitioncontent" in="context.definitioncontent.toString()"/>
			</parameters>
		</function>
		
	</function>
	
	<!--删除：Topology-->
	<function name="RemovePutRequestDocsAPITopology">
		<string name="definitionname"/>
		<string name="definitioncontent"/>
		
		<set name="definitionname" value="'Topology'" />
		
		<function name="removeDefinition" class="APIDocsService">
