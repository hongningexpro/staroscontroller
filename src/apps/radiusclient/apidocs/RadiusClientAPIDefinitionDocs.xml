<starlang xmlns="http://www.staros.xyz/starcore/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" scope="application">
	<function name="DefinitionData_port_sdnmgr">
		<string name="definitionname"/>
		<string name="definitioncontent"/>
		
		<set name="definitionname" value="'Port_sdnmgr'" />
		<reference variable="definitioncontent">
			<jsonbody><![CDATA[
				{
					"type": "object",
					"title": "port",
					"required": [
						"port",
						"workstate",
						"sendbandwidth",
						"receive"
					],
					"properties": {
						"port": {
							"type": "string",
							"example": "0"
						},
						"workstate": {
							"type": "string",
							"example": "active"
						},
						"sendbandwidth": {
							"type": "string",
							"example": "257"
						},
						"receive": {
							"type": "string",
							"example": "354"
						}
					}
				}
			]]></jsonbody>
		</reference>
		
		<function name="addDefinition" class="APIDocsService">
			<parameters>
				<string name="definitionname" in="context.definitionname.toString()"/>
				<string name="definitioncontent" in="context.definitioncontent.toString()"/>
			</parameters>
		</function>
		
	</function>
	
	<function name="RemoveDefinitionData_port_sdnmgr">
		<string name="definitionname"/>
		
		<set name="definitionname" value="'Port_sdnmgr'" />

		<function name="removeDefinition" class="APIDocsService">
			<parameters>
				<string name="definitionname" in="context.definitionname.toString()"/>
			</parameters>
		</function>
	</function>
	<!-- qiulei 20180119 -->
	<function name="DefinitionPortsData">
		<string name="definitionname"/>
		<string name="definitioncontent"/>
		
		<set name="definitionname" value="'Ports'" />
		<reference variable="definitioncontent">
			<jsonbody><![CDATA[
				{
					"type": "object",
					"title": "Ports",
					"required": [
						"ip",
						"ports"
					],
					"properties": {
						"ip": {
							"type": "string",
							"example": "192.168.137.128"
						},
						"type": {
							"type": "array",
							"xml": {
								"name": "ports",
								"wrapped": true
							},
							"items": {
								"type": "object",
								"title": "port",
								"required": [
									"port",
									"workstate",
									"sendbandwidth",
									"receive"
								],
								"properties": {
									"port": {
										"type": "string",
										"example": "0"
									},
									"workstate": {
										"type": "string",
										"example": "active"
									},
									"sendbandwidth": {
										"type": "string",
										"example": "257"
									},
									"receive": {
										"type": "string",
										"example": "354"
									}
								}
							}
						}
					}
				}
			]]></jsonbody>
		</reference>
		
		<function name="addDefinition" class="APIDocsService">
			<parameters>
				<string name="definitionname" in="context.definitionname.toString()"/>
				<string name="definitioncontent" in="context.definitioncontent.toString()"/>
			</parameters>
		</function>
		
	</function>
	
	<function name="RemoveDefinitionPortsData">
		<string name="definitionname"/>
		
		<set name="definitionname" value="'Ports'" />

		<function name="removeDefinition" class="APIDocsService">
