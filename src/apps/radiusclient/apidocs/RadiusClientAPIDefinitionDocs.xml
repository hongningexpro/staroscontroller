	<function name="DefinitionData_devices_sdnmgr">
		<string name="definitionname"/>
		<string name="definitioncontent"/>

		<set name="definitionname" value="'Devices_sdnmgr'" />
		
		<reference variable="definitioncontent">
			<jsonbody><![CDATA[ 
				{
					"type": "object",
					"title": "devices",
					"required": [
						"devices"
					],
					"properties": {
						"devices": {
							"type": "array",
							"xml": {
								"name": "devices",
								"wrapped": true
							},
							"items": {
								"type": "object",
								"title": "device",
								"required": [
									"ip",
									"mac",
									"location",
									"devicetype",
									"devicerole",
									"bandwidth"
								],
								"properties": {
									"ip": {
										"type": "string",
										"example": "192.168.137.128"
									},
									"mac": {
										"type": "string",
										"example": "XX-XX-XX-XX-XX-XX"
									},
									"location": {
										"type": "string",
										"example": "1/3"
									},
									"devicetype": {
										"type": "string",
										"example": "wireless"
									},
									"devicerole": {
										"type": "string",
										"example": "nosoft"
									},
									"bandwidth": {
										"type": "string",
										"example": "100"
									}
								}
							}
						}
					}
				}
			]]></jsonbody>
		</reference>
		
		<function name="addDefinition" class="APIDocsService">
			<parameters>
				<string name="definitionname" in="context.definitionname.toString()"/>
				<string name="definitioncontent" in="context.definitioncontent.toString()"/>
			</parameters>
		</function>
		
	</function>
	
