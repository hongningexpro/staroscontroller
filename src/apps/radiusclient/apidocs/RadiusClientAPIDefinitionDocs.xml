	<!-- 分割线 -->
	<function name="DefinitionPolicyData">
		<string name="definitionname"/>
		<string name="definitioncontent"/>

		<set name="definitionname" value="'Policy_sdnmgr'" />
		<reference variable="definitioncontent">
			<jsonbody><![CDATA[
				{
					"type": "object",
					"title": "policy",
					"required": [
						"policyid",
						"type"
					],
					"properties": {
						"policyid": {
							"type": "string",
							"example": "1"
						},
						"type": {
							"type": "array",
							"xml": {
								"name": "type",
								"wrapped": true
							},
							"items": {
								"type": "object",
								"title": "type",
								"required": [
									"src-devicetype",
									"src-devicerole",
									"dst-devicetype",
									"dst-devicerole",
									"priority"
								],
								"properties": {
									"src-devicetype": {
										"type": "string",
										"example": "wireless"
									},
									"src-devicerole": {
										"type": "string",
										"example": "nosoft"
									},
									"dst-devicetype": {
										"type": "string",
										"example": "wired"
									},
									"dst-devicerole": {
										"type": "string",
										"example": "wired one"
									},
									"priority": {
										"type": "string",
										"example": "0"
									}
								}
							}
						}
					}
				}
			]]></jsonbody>
		</reference>
		
		<function name="addDefinition" class="APIDocsService">
			<parameters>
				<string name="definitionname" in="context.definitionname.toString()"/>
				<string name="definitioncontent" in="context.definitioncontent.toString()"/>
			</parameters>
		</function>
		
	</function>
	
