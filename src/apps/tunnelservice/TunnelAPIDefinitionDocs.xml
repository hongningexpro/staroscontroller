		            "required": [
		                "result"
		            ],
		            "properties": {
		                "result": {
		                    "type": "object",
		                    "title": "result",
		                    "required": [
		                        "resourceId",
						        "code",
						        "description"
		                    ],
		                    "properties": {
		                        "resourceId": {
		                            "type": "string",
		                            "example": "5"
		                        },
		                        "code": {
		                            "type": "string",
