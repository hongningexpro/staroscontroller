		</function>
		
	</function>

	<function name="RemoveDefinitionResultData">
		
		<string name="definitionname"/>

		<set name="definitionname" value="'Result'" />

		<function name="removeDefinition" class="APIDocsService">
			<parameters>
				<string name="definitionname" in="context.definitionname.toString()"/>
			</parameters>
		</function>
		
	</function>






	<!--定义：tunnel -->
	<function name="AddDefinitionTunnelData">
		<string name="definitionname"/>
		<string name="definitioncontent"/>
		
		<set name="definitionname" value="'Tunnel'" />
		
		<reference variable="definitioncontent">
			<jsonbody><![CDATA[ 
				{
					"type": "object",
					"title": "tunnel",
					"required": [
						"tunnel",
						"type",
						"inputPort",
						"outputPort",
						"inputtunnelport",
						"outputtunnelport",
						
						
						"inputvxlanport",
						"outputvxlanport",
						
						
						"inputvlanid",
						"outputvlanid",
						"bandwidth"
					],
					"properties": {
						"username": {
							"type": "string",
							"example": "test"
						},
						"type": {
							"type": "string",
							"example": "vlan"
						},
						"inputPort": {
							"type": "string",
							"example": "of:1234/33"
						},
						"outputPort": {
							"type": "string",
							"example": "of:1234/33"
						},
						"inputtunnelport": {
							"type": "string",
							"example": "of:1234/33"
						},
						"outputtunnelport": {
							"type": "string",
							"example": "of:1234/33"
						},
						
						
						
						
						"inputvxlanport": {
							"type": "string",
							"example": "of:1234/33"
						},
						"outputvxlanport": {
							"type": "string",
							"example": "of:1234/33"
						},
						
						
						
						
						"inputvlanid": {
							"type": "string",
							"example": "12"
						},
						"outputvlanid": {
							"type": "string",
							"example": "12"
						},
						"bandwidth": {
							"type": "string",
							"example": "44333"
						}
					}
				}
			]]></jsonbody>
		</reference>

		<function name="addDefinition" class="APIDocsService">
			<parameters>
				<string name="definitionname" in="context.definitionname.toString()"/>
				<string name="definitioncontent" in="context.definitioncontent.toString()"/>
			</parameters>
		</function>
		
	</function>
	
