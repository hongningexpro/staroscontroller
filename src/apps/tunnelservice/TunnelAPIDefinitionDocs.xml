	<!--定义：state-->
	<function name="AdddefinitionStateData">
		<string name="definitionname"/>
		<string name="definitioncontent"/>
		
		<set name="definitionname" value="'State'" />
		
		<reference variable="definitioncontent">
			<jsonbody><![CDATA[ 
				{
		            "type": "object",
		            "title": "state",
		            "required": [
		                "state"
		            ],
		            "properties": {
