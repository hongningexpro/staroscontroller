	<variable name="ClusterManager" type="class" global="true"/>
	<variable name="DeviceManager" type="class" global="true"/>
	<variable name="HostManager" type="class" global="true"/>
	<variable name="WebSocketManager" type="class" global="true"/>
	<variable name="gInstanceManager" type="class" global="true"/>
	<variable name="gSettingsManager" type="class" global="true"/>
