<?xml version="1.0" encoding="UTF-8"?>
<starosxml xmlns="http://www.staros.xyz/starcore/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" scope="application">
	<function name="OpenFlowMultiPartPortStatsRequest">
		<parameters>
			<parameter name="portno" type="string"/>
			<parameter name="transportid" type="string"/>
			<parameter name="xid" type="long"/>
		</parameters>
		<log dest="stdout">OpenFlowMultiPartPortStatsRequest</log>
		<variable name="strjson" type="string"/>
		<reference variable="strjson">
			<jsonbody><![CDATA[ 
					{
						"xid":"<%=context.xid.toString()%>",
						"portno":"<%=context.portno.toString()%>"
					}
			]]></jsonbody>
		</reference>
		<function name="Send_OFPT13_MULTIPART_REQUEST.OFP13MP_PORT_STATS">
			<parameters>
				<parameter name="transportid" type="string" in="context.transportid.toString()"/>
				<parameter name="strjson" type="string" in="context.strjson.toString()"/>
			</parameters>
		</function>
	</function>
	<function name="OpenFlowMultiPartPortStatsReply">
		<parameters>
			<parameter name="xid" type="long"/>
		</parameters>
		<log dest="stdout">OpenFlowMultiPartPortStatsReply</log>
		<variable name="strjson" type="string"/>
		<reference variable="strjson">
			<jsonbody><![CDATA[ 
					{
						"xid":"<%=context.xid.toLong()%>",
						"portstatslist":[
							{
								"portno":"1",
								"rxpackets":"1",
								"txpackets":"1",
								"rxbytes":"1",
								"txbytes":"1",
								"rxdropped":"1",
								"txdropped":"1",
								"rxerrors":"1",
								"txerrors":"1",
								"rxframeerr":"1",
								"rxovererr":"1",
								"rxcrcerr":"1",
								"collisions":"1",
								"durationsec":"1",
								"durationnsec":"1"
							},
							{
								"portno":"1",
								"rxpackets":"1",
								"txpackets":"1",
								"rxbytes":"1",
								"txbytes":"1",
								"rxdropped":"1",
								"txdropped":"1",
								"rxerrors":"1",
								"txerrors":"1",
								"rxframeerr":"1",
								"rxovererr":"1",
								"rxcrcerr":"1",
								"collisions":"1",
								"durationsec":"1",
								"durationnsec":"1"
							}
						]
					}
			]]></jsonbody>
		</reference>
		<function name="Send_OFPT13_MULTIPART_REPLY.OFP13MP_PORT_STATS">
			<parameters>
				<parameter name="transportid" type="string" in="event.getParam('transportid').toString()"/>
				<parameter name="strjson" type="string" in="context.strjson.toString()"/>
			</parameters>
		</function>
	</function>
	<function name="TestOpenFlowMultiPartPortStatsRequest">
		<variable name="isstop" type="boolean" value="true"/>
		<variable name="xid" type="long"/>
		<set name="xid" value="context.openflowserverobj.getIncreaseXID()"/>
		<log><![CDATA[TestOpenFlowMultiPartGroupDescRequest xid=<%=context.xid.toLong()%>]]></log>
		<function>
			<function name="OpenFlowMultiPartPortStatsRequest">
				<parameters>
					<parameter name="portno" type="string"/>
				<parameter name="transportid" type="string"/>
				<parameter name="xid" type="long" />
				</parameters>
			</function>
			<wait event="openflow.OFPT13_MULTIPART_REPLY.OFP13MP_PORT_STATS,openflow.OFPT13_ERROR" transportid="event.getParam('transportid').toString()" xid="context.xid.toLong()">
				<function>
					<log><![CDATA[OFPT13_MULTIPART_REPLY.OFP13MP_PORT_STATS=<%=event.getParam('eventdata').toString()%>]]></log>
				</function>
			</wait>
		</function>
	</function>
	<function name="TestOpenFlowMultiPartPortStatsReply">
		<variable name="isstop" type="boolean" value="true"/>
		<variable name="xid" type="long"/>
		<set name="xid" value="context.openflowserverobj.getIncreaseXID()"/>
		<log><![CDATA[TestOpenFlowMultiPartPortStatsReply xid=<%=context.xid.toLong()%>]]></log>
		<function>
			<function name="OpenFlowMultiPartPortStatsReply">
				<parameters>
					<parameter name="xid" type="long" in="context.xid.toLong()"/>
				</parameters>
			</function>
		</function>
	</function>
</starosxml>