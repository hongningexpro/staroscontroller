	<!--29-0 send HELLO message, 
		strjson example:
		
		{
		"xid":"1",
		"elementtype":"1",
		"elementlength":"1",
		"elementbitmap":"1"
		}
		
	qiulei, 2017/6/19 -->
	
	<function name="Send_OFPT13_HELLO">
		<parameters>
			<parameter name="transportid" type="string"/>
			<parameter name="strjson" type="string"/>
		</parameters>
		<variable name="openflowparams" type="params"/>
        <log dest="stdout">Send_OFPT13_HELLO</log>
		<log><![CDATA[Send_OFPT13_HELLO=<%=context.strjson.toString%>]]></log>
		<set name="openflowparams" paramname="transportid" value="context.transportid"/>
		<openflow name="openflowserverobj" method="openflow.OFPT13_HELLO" params="context.openflowparams">
			<jsonsrc src="strjson"/>
		</openflow>
	</function>
