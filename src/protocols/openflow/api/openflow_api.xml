	<!--29-0 send HELLO message, 
		strjson example:
		
		{
		"xid":"1",
		"elementtype":"1",
		"elementlength":"1",
		"elementbitmap":"1"
		}
		
	qiulei, 2017/6/19 -->
	
	<function name="Send_OFPT13_HELLO">
		<parameters>
			<string name="transportid" />
			<string name="strjson" />
		</parameters>
		<params name="openflowparams"/>
        <log>Send_OFPT13_HELLO</log>
		<set name="openflowparams" paramname="transportid" value="context.transportid"/>
		<openflow name="openflowserverobj" method="openflow.OFPT13_HELLO" params="context.openflowparams">
			<jsonsrc src="strjson"/>
		</openflow>
	</function>
