<?xml version="1.0" encoding="UTF-8"?>
<starosxml xmlns="http://www.staros.xyz/starcore/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" scope="application">
	<function name="OpenFlowMultiPartQueueRequest">
		<parameters>
			<parameter name="xid" type="long"/>
		</parameters>
		<log>OpenFlowMultiPartQueueRequest</log>
		<variable name="strjson" type="string"/>
		<reference variable="strjson">
			<jsonbody><![CDATA[ 
					{
						"xid":"<%=context.xid.toLong()%>",
						"portno":"1",
						"queueid":"1"
					}
			]]></jsonbody>
		</reference>
		<function name="Send_OFPT13_MULTIPART_REQUEST.OFP13MP_QUEUE">
			<parameters>
				<parameter name="transportid" type="string" in="event.getParam('transportid').toString()"/>
				<parameter name="strjson" type="string" in="context.strjson.toString()"/>
			</parameters>
		</function>
	</function>
	<function name="OpenFlowMultiPartQueueReply">
		<parameters>
			<parameter name="xid" type="long"/>
		</parameters>
		<log>OpenFlowMultiPartQueueReply</log>
		<variable name="strjson" type="string"/>
		<reference variable="strjson">
			<jsonbody><![CDATA[ 
					{
						"xid":"<%=context.xid.toLong()%>",
						"portstatslist":[
							{
								"portno":"1",
								"queueid":"1",
								"txbytes":"1",
								"txpackets":"1",
								"txerrors":"1",
								"durationsec":"1",
								"durationnsec":"1"
							},
							{
								"portno":"1",
								"queueid":"1",
								"txbytes":"1",
								"txpackets":"1",
								"txerrors":"1",
								"durationsec":"1",
								"durationnsec":"1"
							}
						]
					}
			]]></jsonbody>
		</reference>
		<function name="Send_OFPT13_MULTIPART_REPLY.OFP13MP_QUEUE">
			<parameters>
				<parameter name="transportid" type="string" in="event.getParam('transportid').toString()"/>
				<parameter name="strjson" type="string" in="context.strjson.toString()"/>
			</parameters>
		</function>
	</function>
	<function name="TestOpenFlowMultiPartQueueRequest">
		<variable name="isstop" type="boolean" value="true"/>
		<variable name="xid" type="long"/>
		<set name="xid" value="context.openflowserverobj.getIncreaseXID()"/>
		<log><![CDATA[TestOpenFlowMultiPartGroupDescRequest xid=<%=context.xid.toLong()%>]]></log>
		<function>
			<function name="OpenFlowMultiPartQueueRequest">
				<parameters>
					<parameter name="xid" type="long" in="context.xid.toLong()"/>
				</parameters>
			</function>
			<wait event="openflow.OFPT13_MULTIPART_REPLY.OFP13MP_QUEUE,openflow.OFPT13_ERROR" transportid="event.getParam('transportid').toString()" xid="context.xid.toLong()">
				<function>
					<log><![CDATA[OFPT13_MULTIPART_REPLY.OFP13MP_QUEUE=<%=event.getParam('eventdata').toString()%>]]></log>
				</function>
			</wait>
		</function>
	</function>
	<function name="TestOpenFlowMultiPartQueueReply">
		<variable name="isstop" type="boolean" value="true"/>
		<variable name="xid" type="long"/>
		<set name="xid" value="context.openflowserverobj.getIncreaseXID()"/>
		<log><![CDATA[TestOpenFlowMultiPartQueueReply xid=<%=context.xid.toLong()%>]]></log>
		<function>
			<function name="OpenFlowMultiPartQueueReply">
				<parameters>
					<parameter name="xid" type="long" in="context.xid.toLong()"/>
				</parameters>
			</function>
		</function>
	</function>
</starosxml>