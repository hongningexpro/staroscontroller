<?xml version="1.0" encoding="UTF-8"?>
<starlang xmlns="http://www.staros.xyz/starcore/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" scope="application">
	<function name="OpenFlowMultiPartMeterConfigRequest">
		<parameters>
			<long name="xid"/>
		</parameters>
		<log>OpenFlowMultiPartMeterConfigRequest</log>
		<string name="strjson"/>
		<reference variable="strjson">
			<jsonbody><![CDATA[ 
					{
						"xid":"<%=context.xid.toString()%>",
						"meterid":"0xffffffff"
					}
			]]></jsonbody>
		</reference>
		<function name="Send_OFPT13_MULTIPART_REQUEST.OFP13MP_METER_CONFIG">
			<parameters>
				<string name="transportid" in="event.getParam('transportid').toString()"/>
				<string name="strjson" in="context.strjson.toString()"/>
			</parameters>
		</function>
	</function>
	<function name="OpenFlowMultiPartMeterConfigReply">
		<parameters>
			<long name="xid"/>
		</parameters>
		<log>OpenFlowMultiPartMeterConfigReply</log>
		<string name="strjson"/>
		<reference variable="strjson">
			<jsonbody><![CDATA[ 
				{
					"flags" : "0",
					"meterconfiglist" : [
						{
							"flags":"1",
							"meterid":"1",
							"meterbandlist":[
								{
									"bandtype":"1",
									"rate":"11",
									"burstsize":"1024"
								},
								{
									"bandtype":"2",
									"rate":"11",
									"burstsize":"1024",
									"preclevel":"34"
								}
							]
						}
					],
					"type" : "10",
					"xid":"<%=context.xid.toString()%>"
				}
			]]></jsonbody>
		</reference>
		<function name="Send_OFPT13_MULTIPART_REPLY.OFP13MP_METER_CONFIG">
			<parameters>
				<string name="transportid" in="event.getParam('transportid').toString()"/>
				<string name="strjson" in="context.strjson.toString()"/>
			</parameters>
		</function>
	</function>
	<function name="TestOpenFlowMultiPartMeterConfigRequest">
		<boolean name="isstop" value="true"/>
		<long name="xid"/>
		<set name="xid" value="context.openflowserverobj.ptr.getIncreaseXID()"/>
		<function>
			<function name="OpenFlowMultiPartMeterConfigRequest">
				<parameters>
					<long name="xid" in="context.xid.toLong()"/>
				</parameters>
			</function>
			<wait event="openflow.OFPT13_MULTIPART_REPLY.OFP13MP_METER_CONFIG,openflow.OFPT13_ERROR" transportid="event.getParam('transportid').toString()"  xid="context.xid.toLong()">
				<function>
					<log><![CDATA[OFPT13_MULTIPART_REPLY.OFP13MP_METER_CONFIG=<%=event.getParam('eventdata').toString()%>]]></log>
				</function>
			</wait>
		</function>
	</function>
	<function name="TestOpenFlowMultiPartMeterConfigReply">
		<long name="xid"/>
		<set name="xid" value="context.openflowserverobj.ptr.getIncreaseXID()"/>
		<function>
			<function name="OpenFlowMultiPartMeterConfigReply">
				<parameters>
					<long name="xid" in="context.xid.toLong()"/>
				</parameters>
			</function>
		</function>
	</function>
</starlang>