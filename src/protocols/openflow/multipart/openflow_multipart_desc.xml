<?xml version="1.0" encoding="UTF-8"?>
<starosxml xmlns="http://www.staros.xyz/starcore/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" scope="application">
	<function name="OpenFlowMultiPartDescRequest">
		<parameters>
			<parameter name="xid" type="long"/>
		</parameters>
		<log dest="stdout">OpenFlowMultiPartDescRequest</log>
		<variable name="strjson" type="string"/>
		<reference variable="strjson">
			<jsonbody><![CDATA[ 
					{
						"xid":"<%=context.xid.toLong()%>"
					}
			]]></jsonbody>
		</reference>
		<function name="Send_OFPT13_MULTIPART_REQUEST.OFP13MP_DESC">
			<parameters>
				<parameter name="transportid" type="string" in="event.getParam('transportid').toString()"/>
				<parameter name="strjson" type="string" in="context.strjson.toString()"/>
			</parameters>
		</function>
	</function>
	<function name="OpenFlowMultiPartDescReply">
		<parameters>
			<parameter name="xid" type="long"/>
		</parameters>
		<log dest="stdout">OpenFlowMultiPartDescReply</log>
		<variable name="strjson" type="string"/>
		<reference variable="strjson">
			<jsonbody><![CDATA[ 
					{
						"xid":"<%=context.xid.toLong()%>",
						"mfrdesc":"1",
						"swdesc":"1",
						"serialnum":"1",
						"dpdesc":"1",
						"hwdesc":"1"
					}
			]]></jsonbody>
		</reference>
		<function name="Send_OFPT13_MULTIPART_REPLY.OFP13MP_DESC">
			<parameters>
				<parameter name="transportid" type="string" in="event.getParam('transportid').toString()"/>
				<parameter name="strjson" type="string" in="context.strjson.toString()"/>
			</parameters>
		</function>
	</function>
	<function name="TestOpenFlowMultiPartDescRequest">
		<variable name="isstop" type="boolean" value="true"/>
		<variable name="xid" type="long"/>
		<set name="xid" value="context.openflowserverobj.getIncreaseXID()"/>
		<log><![CDATA[TestOpenFlowMultiPartGroupDescRequest xid=<%=context.xid.toLong()%>]]></log>
		<function>
			<function name="OpenFlowMultiPartDescRequest">
				<parameters>
					<parameter name="xid" type="long" in="context.xid.toLong()"/>
				</parameters>
			</function>
			<wait event="openflow.OFPT13_MULTIPART_REPLY.OFP13MP_DESC,openflow.OFPT13_ERROR" transportid="event.getParam('transportid').toString()" xid="context.xid.toLong()">
				<function>
					<log><![CDATA[OFPT13_MULTIPART_REPLY.OFP13MP_DESC=<%=event.getParam('eventdata').toString()%>]]></log>
				</function>
			</wait>
		</function>
	</function>
	<function name="TestOpenFlowMultiPartDescReply">
		<variable name="isstop" type="boolean" value="true"/>
		<variable name="xid" type="long"/>
		<set name="xid" value="context.openflowserverobj.getIncreaseXID()"/>
		<log><![CDATA[TestOpenFlowMultiPartDescReply xid=<%=context.xid.toLong()%>]]></log>
		<function>
			<function name="OpenFlowMultiPartDescReply">
				<parameters>
					<parameter name="xid" type="long" in="context.xid.toLong()"/>
				</parameters>
			</function>
		</function>
	</function>
</starosxml>