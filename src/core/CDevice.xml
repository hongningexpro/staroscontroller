							payload['propOrder'] = oneArray;
							payload['props'] = twoarray;
							payload['buttons'] = threeArray;
						
						var json = new Object();						
							json['event'] = "showDetails";
							json['payload'] = payload;
							
						var strjson=JSON.stringify(json);
						context.detailarray.setValue(strjson);
					
					]]></script>
