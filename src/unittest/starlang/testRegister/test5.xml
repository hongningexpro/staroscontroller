			<syslog level="WARNING">---WARNING:enter SendMessage   2017.12.12</syslog>
			<syslog level="FATAL">---FATAL:enter SendMessage       2017.12.12</syslog>
			
			<systemgui name="context.obj123" action="SendMessage">
				<jsonbody><![CDATA[
					{
						"faultStatistics":[
							{
								"id":"183aa",
								"user":"John Doe",
								"date":"11-7-2014",
