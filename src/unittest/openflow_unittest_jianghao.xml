<?xml version="1.0" encoding="UTF-8"?>
<starosxml xmlns="http://www.staros.xyz/starcore/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" scope="application">
	<function name="TestMPMeterRequest">
		<log>"Send_OFPT13_MULTIPART_REQUEST.OFP13MP_METER"</log>
		<function name="Send_OFPT13_MULTIPART_REQUEST.OFP13MP_METER">			
			<parameters>
				<parameter name="transportid" type="string" in="event.getParam('transportid').toString()"/>
				<parameter name="xid" type="long" in="context.openflowxidbase.toLong()+1"/>
				<parameter name="meterid" type="string" in="'5'"/>
			</parameters>
		</function>
		<wait event="openflow.OFPT13_MULTIPART_REPLY.OFP13MP_TABLE" timeout="1000">
			<function>
				<log><![CDATA[event name=<%=event.getName().toString()%>]]></log>
				<log><![CDATA[event data=<%=event.getParam('eventdata').toString()%>]]></log>
			</function>
		</wait>
	</function>
	<!--jianghao-->
	<function name="TestFLOWMOD">
		<log>"SEND_OFPT13_FLOW_MOD IN"</log>
		<function name="Send_OFPT13_FLOW_MOD">
			<parameters>
				<parameter name="transportid" type="string" in="event.getParam('transportid').toString()"/>
				<parameter name="xid" type="long" in="context.openflowxidbase.toLong()+1"/>
				<parameter name="cookie" type="string" in="'0'"/>
				<parameter name="cookiemask" type="string" in="'0'"/>
				<parameter name="tableid" type="string" in="'0'"/>
				<parameter name="command" type="string" in="'0'"/>
				<parameter name="idletimeout" type="string" in="'0'"/>
				<parameter name="hardtimeout" type="string" in="'0'"/>
				<parameter name="priority" type="string" in="'0'"/>
				<parameter name="bufferid" type="string" in="'0'"/>
				<parameter name="outport" type="string" in="'1'"/>
				<parameter name="outgroup" type="string" in="'0'"/>
				<parameter name="flags" type="string" in="'0'"/>
				<parameter name="instructionlist" type="string" in="'0'"/>
			</parameters>
		</function>
		<log>"SEND_OFPT13_FLOW_MOD OUT"</log>
	</function>
</starosxml>