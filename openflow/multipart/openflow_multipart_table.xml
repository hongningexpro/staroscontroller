<?xml version="1.0" encoding="UTF-8"?>
<starosxml xmlns="http://www.staros.xyz/starcore/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" scope="application">
	<function name="OpenFlowMultiPartTableRequest">
		<parameters>
			<parameter name="xid" type="long"/>
		</parameters>
		<log dest="stdout">OpenFlowMultiPartTableRequest</log>
		<variable name="strjson" type="string"/>
		<reference variable="strjson">
			<jsonbody><![CDATA[ 
					{
						"xid":"<%=context.xid.toLong()%>"
					}
			]]></jsonbody>
		</reference>
		<function name="Send_OFPT13_MULTIPART_REQUEST.OFP13MP_TABLE">
			<parameters>
				<parameter name="transportid" type="string" in="event.getParam('transportid').toString()"/>
				<parameter name="strjson" type="string" in="context.strjson.toString()"/>
			</parameters>
		</function>
	</function>
	<function name="OpenFlowMultiPartTableReply">
		<parameters>
			<parameter name="xid" type="long"/>
		</parameters>
		<log dest="stdout">OpenFlowMultiPartTableReply</log>
		<variable name="strjson" type="string"/>
		<reference variable="strjson">
			<jsonbody><![CDATA[ 
					{
						"xid":"<%=context.xid.toLong()%>",
						"tablestatslist":[
							{								
								"tableid":"1",
								"activecount":"1",
								"lookupcount":"1",
								"matchedcount":"1"
							},
							{
								"tableid":"2",
								"activecount":"2",
								"lookupcount":"2",
								"matchedcount":"2"
							}
						]
					}
			]]></jsonbody>
		</reference>
		<function name="Send_OFPT13_MULTIPART_REPLY.OFP13MP_TABLE">
			<parameters>
				<parameter name="transportid" type="string" in="event.getParam('transportid').toString()"/>
				<parameter name="strjson" type="string" in="context.strjson.toString()"/>
			</parameters>
		</function>
	</function>
	<function name="TestOpenFlowMultiPartTableRequest">
		<variable name="isstop" type="boolean" value="true"/>
		<variable name="xid" type="long"/>
		<set name="xid" value="context.openflowserverobj.getIncreaseXID()"/>
		<log><![CDATA[TestOpenFlowMultiPartTableRequest xid=<%=context.xid.toLong()%>]]></log>
		<function>
			<function name="OpenFlowMultiPartTableRequest">
				<parameters>
					<parameter name="xid" type="long" in="context.xid.toLong()"/>
				</parameters>
			</function>
			<wait event="openflow.OFPT13_MULTIPART_REPLY.OFP13MP_TABLE,openflow.OFPT13_ERROR" transportid="event.getParam('transportid').toString()" xid="context.xid.toLong()">
				<function>
					<log><![CDATA[OFPT13_MULTIPART_REPLY.OFP13MP_TABLE=<%=event.getParam('eventdata').toString()%>]]></log>
				</function>
			</wait>
		</function>
	</function>
	<function name="TestOpenFlowMultiPartTableReply">
		<variable name="isstop" type="boolean" value="true"/>
		<variable name="xid" type="long"/>
		<set name="xid" value="context.openflowserverobj.getIncreaseXID()"/>
		<log><![CDATA[TestOpenFlowMultiPartQueueReply xid=<%=context.xid.toLong()%>]]></log>
		<function>
			<function name="OpenFlowMultiPartTableReply">
				<parameters>
					<parameter name="xid" type="long" in="context.xid.toLong()"/>
				</parameters>
			</function>
		</function>
	</function>
</starosxml>